[Default]
daemon=false
pidfile=<%= @localstatedir %>/rawx-<%= @num %>-monitor.pid

[Child]
command=/usr/sbin/httpd.worker -D FOREGROUND -f <%= @sysconfdir %>/rawx-<%= @num %>-httpd.conf
respawn=true
rlimit.stack_size=1048576
rlimit.core_size=-1
rlimit.max_files=32768

[Service]
ns=<%= @options["ns"] %>
type=rawx
location=<%= @options["server"] %>
<% if @options["stgclass"] and @options["stgclass"] != '' %>
stgclass=<%= @options["stgclass"] %>
<% else %>
stgclass=NORMAL
<% end %>
addr=<%= @options["ipaddr"] %>:<%= @options["port"] %>

[Volume]
docroot=<%= @datadir %>
filer.user=root
filer.pwd=root
